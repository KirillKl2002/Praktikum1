import random
M = random.randint(1,10)
arr = [random.randint(1,10) for i in range (M)]
for i in range (M):
    arr[i]= input()
for i in range (M):
    x = 1
    for n in range (len(arr[i]))
    S = arr[i]
    if S[n] == " ":
        x += 1
    print (" количество слогов в слове " + "\"" + str(arr[i])+ "\""
           + " равно" + str(x))
